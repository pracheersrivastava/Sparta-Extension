{
    "name": "SPARTA Detection",
    "version": "1.0.0",
    "description": "An advanced phishing detection plugin",
    "permissions": ["activeTab", "declarativeContent", "storage", "webNavigation"],
    "background": {
      "service_worker": "js/bg-loader.js"
    },
    "action": {
      "default_popup": "plugin_ui.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["js/features.js"]
      }
    ],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "manifest_version": 3
  }
  